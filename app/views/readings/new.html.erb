<h1 style="color: green"> hello, reading</h1>

<%= form_for :reading, url: '/readings' do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :'start date' %><br>
    <%= f.text_field :startDate %><br>
    <%= f.label :'end date' %><br>
    <%= f.text_field :endDate %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
